{
  "name": "wandermate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prisma generate --schema=./prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "node setup-apis.js",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@google-cloud/translate": "^9.1.0",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@pinecone-database/pinecone": "^6.1.0",
    "@prisma/client": "^6.9.0",
    "@react-google-maps/api": "^2.20.6",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.17",
    "@upstash/redis": "^1.35.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "date-fns": "^4.1.0",
    "elevenlabs-node": "^2.0.3",
    "firebase": "^11.9.0",
    "firebase-admin": "^12.7.0",
    "framer-motion": "^12.16.0",
    "langchain": "^0.1.37",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^4.104.0",
    "prisma": "^6.9.0",
    "razorpay": "^2.9.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "resend": "^4.5.2",
    "sharp": "^0.34.3",
    "stripe": "^18.2.1",
    "three": "^0.179.1",
    "twilio": "^5.7.0",
    "uuid": "^9.0.1",
    "vector-storage": "^1.0.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
